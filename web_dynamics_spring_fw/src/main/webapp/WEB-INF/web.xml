<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>web_dynamics</display-name>
  <welcome-file-list>
    <welcome-file>frontServlet</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>myserv</servlet-name>
    <servlet-class>com.nitesh.start_serv.FrontServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>myserv</servlet-name>
    <url-pattern>/frontServlet</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>signupServlet</servlet-name>
    <servlet-class>com.nitesh.start_serv.SignUpServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>signupServlet</servlet-name>
    <url-pattern>/resources/jsp/signupServlet</url-pattern>
  </servlet-mapping>
</web-app>